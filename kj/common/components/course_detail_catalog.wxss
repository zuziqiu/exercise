.details_catalog_content-v2 {
  position: relative;
}
.details_catalog_content-v2 .item {
  padding: 3.125% 3.75% 2.5%;
  display: block;
  overflow: hidden;
  position: relative;
  border-bottom: 1px solid #eee;
  text-decoration: none;
}
.details_catalog_content-v2 .item .item_img_container {
  width: 35.48%;
  float: left;
  line-height: 0;
  overflow: hidden;
  position: relative;
  background-color: blue;
}
.details_catalog_content-v2 .item .item_img_container .item_img {
  width: 100%;
  overflow: hidden;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.details_catalog_content-v2 .item .item_img_container .icon_container {
  width: 20.32%;
  height: 30.5%;
  background: rgba(0, 0, 0, 0.7);
  border-radius: 50%;
  display: block;
  position: absolute;
  bottom: 3.260%;
  right: 3.260%;
}
.details_catalog_content-v2 .item .item_img_container .icon_container .item_icon {
  width: 100%;
  height: 100%;
  display: block;
  background-repeat: no-repeat;
  background-size: 15px 10px;
  background-position: 50% 50%;
}
.details_catalog_content-v2 .item .item_img_container .icon_container.hidden {
  display: none;
}
.details_catalog_content-v2 .item .auto_w {
  width: 58%;
  padding: 0 0 0 3.380%;
  overflow: hidden;
}
.details_catalog_content-v2 .item .auto_w .courseName {
  width: 100%;
  color: #333333;
  font-size: 15px;
  line-height: 20px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.details_catalog_content-v2 .item .auto_w .course_details {
  position: absolute;
  z-index: 2;
  width: 54%;
  bottom: 0%;
  padding-bottom: 2.5%;
}
.details_catalog_content-v2 .item .auto_w .course_details .time {
  color: #999;
  font-size: 14px;
  float: left;
}
.details_catalog_content-v2 .item .auto_w .course_details .time.time_tip {
  font-size: 13px;
  color: #ffa113;
  position: relative;
  margin-left: 6px;
}
.details_catalog_content-v2 .item .auto_w .course_details .time.time_tip:before {
  content: "Â·";
  position: absolute;
  font-size: 30px;
  top: -9px;
  left: -12px;
}
.details_catalog_content-v2 .item .auto_w .course_details .price {
  color: #f34747;
  font-size: 15px;
  float: right;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.tran {
  color: #3ac9ab;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.stop {
  color: #999;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.icon {
  width: 11.5px;
  height: 15.5px;
  margin: 0;
  float: right;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.icon:after {
  content: "";
  width: 11.5px;
  display: block;
  height: 15.5px;
  background-size: 11.5px 15.5px;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.study {
  color: #3ac9ab;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.live {
  color: #ffb04a;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.not {
  color: #aaa;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.hidden {
  display: none;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.mc_tryView {
  color: #000;
  width: 48px;
  height: 22px;
  font-size: 12px;
  margin-top: -8px;
  line-height: 22px;
  position: relative;
  text-align: center;
  padding-left: 12px;
  border-radius: 22px;
  border: 1px solid #fcd358;
}
.details_catalog_content-v2 .item .auto_w .course_details .price.mc_tryView:before {
  top: 50%;
  left: 8px;
  width: 7px;
  height: 7px;
  content: '';
  margin-top: -4px;
  position: absolute;
  background-size: 7px;
  background-repeat: no-repeat;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type {
  line-height: 34px;
  font-size: 14px;
  color: #3ac9ab;
  float: right;
  display: block;
  padding-right: 6.370%;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.icon {
  width: 11.5px;
  height: 15.5px;
  margin: 0;
  float: right;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.icon:after {
  content: "";
  width: 11.5px;
  display: block;
  height: 15.5px;
  background-size: 11.5px 15.5px;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.study {
  color: #3ac9ab;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.live {
  color: #ffb04a;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.not {
  color: #aaa;
  font-size: 14px;
}
.details_catalog_content-v2 .item .auto_w .course_details .live_type.hidden {
  display: none;
}
.change_background {
  -webkit-animation-name: change_background;
  -webkit-animation-duration: 200ms;
  -webkit-animation-iteration-count: 1;
}
@-webkit-keyframes change_background {
  0% {
    background-color: #ffffff;
  }
  100% {
    background-color: #efefef;
  }
}
