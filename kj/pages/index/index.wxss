}
.main .section_banner {
  width: 100%;
  /*height:14rem/2;*/
  background: url(../images/banner.png);
  background-size: 100% 100%;
}
.main .section_banner image {
  width: 100%;
  height: 100%;
  display: block;
  /*background:#C5C5CF;*/
}
.main .section_course {
  width: 100%;
  margin-top: 15px;
  background: #fff;
  overflow: hidden;
}
.main .section_course .block_course_header {
  width: 95%;
  padding: 15px 0px 0px;
  float: right;
  overflow: hidden;
}
.main .section_course .block_course_header .course_header_title {
  font-size: 0.8rem;
  color: #000;
  font-family: 'FZLTXIHJW--GB1-0';
  display: block;
  float: left;
}
.main .section_course .block_course_header .course_header_whole {
  display: block;
  float: right;
  padding-right: 1rem;
}
.main .section_course .block_course_header .course_header_whole a {
  font-size: 0.6rem;
  color: #666;
  text-decoration: none;
  display: block;
}
.main .section_course .block_course_main {
  width: 95%;
  float: right;
  overflow: hidden;
}
.main .section_course .block_course_main .course_main_container {
  width: 100%;
  border-bottom: 1px solid #ddd;
  overflow: hidden;
}
.main .section_course .block_course_main .course_main_container .course_main_content {
  width: 100%;
  padding: 15px 0px;
  display: block;
  overflow: hidden;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover {
  width: 80px;
  height: 60px;
  border-radius: 5px;
  background: #8292C3;
  float: left;
  position: relative;
  overflow: hidden;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover image {
  width: 100%;
  height: 100%;
  display: block;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover .cover_icon {
  width: 20px;
  height: 20px;
  display: block;
  position: absolute;
  right: 0.25rem;
  bottom: 0.25rem;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.3);
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover .cover_icon.hidden {
  display: none;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover .cover_icon.video span {
  width: 13.5px;
  height: 8px;
  display: block;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: url(../images/course_icon_02.png);
  background-size: 100% 100%;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_cover .cover_icon.audio span {
  width: 10.5px;
  height: 8px;
  display: block;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: url(../images/course_icon_03.png);
  background-size: 100% 100%;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info {
  padding: 0rem 1rem 0rem 1.5rem;
  overflow: hidden;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info .course_info_title {
  font-size: 0.8rem;
  color: #000;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info .course_info_intr {
  font-size: 0.6rem;
  color: #666;
  display: block;
  margin-top: 7px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info .course_info_bottom {
  overflow: hidden;
  /*line-height:50px/2;*/
  margin-top: 0.65rem;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info .course_info_bottom .course_info_num {
  font-size: 0.6rem;
  color: #666;
  float: left;
  display: inline-block;
}
.main .section_course .block_course_main .course_main_container .course_main_content .course_info .course_info_bottom .course_info_free {
  font-size: 0.6rem;
  color: #f00;
  display: inline-block;
  float: right;
}
.main .section_course .block_course_main .course_main_container:last-child {
  border-bottom: 0;
}
