.catalog_navigation .catalog_content {
  width: 100%;
  height: 44px;
  line-height: 44px;
  justify-content: space-around;
  border-bottom: 1px solid #f5f5f5;
}
.catalog_navigation .catalog_content .catalog_target {
  flex: 1;
  color: #666;
  text-align: center;
  font-size: 0.85rem;
}
.catalog_navigation .catalog_content .catalog_target.selected {
  color: #000;
  box-shadow: 0 -2px 0 0 #FCD358 inset;
}
